
@{
    ViewData["Title"] = "Index";
}



<div class="container">
    @* Listado *@
    <div class="card">
        <div class="card-header">
            <div class="row">
                <div class="col-md-12"><h3>Listado de Vacunas</h3></div>
                <div class="col-md-2">
                </div>
            </div>
        </div>

        <div class="card-body">
            <table class="table table-striped dt-responsive nowrap " id="datatable">
                <thead class="text-center bg-primary text-white">
                    <tr>
                        <th>Fila</th>
                        <th>Descripción</th>
                        <th>Acciones</th>
                    </tr>
                </thead>
            </table>

        </div>
    </div>
</div>